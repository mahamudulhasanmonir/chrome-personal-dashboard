{
  "manifest_version": 3,
  "name": "My Smart Dashboard",
  "version": "1.0",
  "description": "Dashboard with notes, weather, reminders, and theme toggle",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "permissions": ["notifications"],
  "host_permissions": ["https://api.openweathermap.org/"]
}
